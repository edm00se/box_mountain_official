<section class="cryptid-box-nav">
    <div class="cryptid-box-nav-wrapper" id="cryptidTopnav">
        <a href="{{section.settings.cryptid_box_nav_logo_link}}"><img class="cryptid-box-nav-logo" src="{{section.settings.cryptid_box_nav_logo | img_url: '70x'}}"></a>
        <a class="cryptid-box-nav-link" href="{{section.settings.cryptid_nav_plans}}">Plans</a>
        <a class="cryptid-box-nav-link" href="{{section.settings.cryptid_nav_faq}}">FAQ</a>
        <a class="cryptid-box-nav-link" href="{{section.settings.cryptid_nav_inside_box}}">What's Inside</a>
        <a class="icon-cryptid" href="javascript:void(0);" onclick="myFunctionCryptid()">
            <i class="fa fa-bars" id="cryptid-fa-bars"></i>
        </a>
    </div>
</section>

{% schema %}
{
    "name": "Cryptid Box Nav",
    "settings": [
        {
            "type": "image_picker",
            "id": "cryptid_box_nav_logo",
            "label": "Cryptid Box Nav Logo"
        },
        {
            "type": "url",
            "id": "cryptid_box_nav_logo_link",
            "label": "Cryptid Box Nav Logo Link"
        },{
            "type": "url",
            "id":"cryptid_nav_plans",
            "label": "Cryptid Nav Plans Link"
        },
        {
            "type": "url",
            "id":"cryptid_nav_faq",
            "label": "Cryptid Nav FAQ"
        },
        {
            "type": "url",
            "id":"cryptid_nav_inside_box",
            "label": "Cryptid Nav Inside the Box"
        }
    ]
}

{% endschema %}